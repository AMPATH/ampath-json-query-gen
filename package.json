{
  "name": "ampath-json-query-gen",
  "version": "1.0.0-alpha3",
  "description": "Generate dynamic json representation of a mysql query, compatible with ampath-json2sql",
  "main": "lib/library.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "nyc --reporter=html --reporter=text --check-coverage --lines 90 --per-file mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AMPATH/ampath-json-query-gen.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "AMPATH Devs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AMPATH/ampath-json-query-gen/issues"
  },
  "homepage": "https://github.com/AMPATH/ampath-json-query-gen",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chai-string": "^1.4.0",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "sinon": "^4.4.6",
    "sinon-chai": "^3.0.0",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "ampath-json2sql": "git+https://github.com/AMPATH/ampath-json2sql.git#v1.0.0",
    "lodash": "^4.17.5",
    "mocha-logger": "^1.0.5",
    "nyc": "^11.4.1",
    "squel": "^5.12.0"
  }
}
